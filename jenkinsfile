@Library('mylibrary')_
pipeline
{
    agent any
    stages
    {
        stage('continuousDownload_master')
        {
            steps
            {
                script
                {
                    cicd.newgit("dellrep")
                }
            }
        }
        stage('continuousbuild_master')
        {
            steps
            {
                script
                {
                    cicd.newmaven()
                }
            }
        }
        stage('continuousdeployment_master')
        {
            steps
            {
                script
                {
                    cicd.newdeploy("cicdshared","172.31.94.8","testapp")
                }
            }
        }
        stage('continuoustesting_master')
        {
            steps
            {
                script
                {
                    cicd.newgit("testing")
                    cicd.newtest("cicdshared")
                }
            }
        }
    }
}
